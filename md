// code for odd and even numbers.
#include<stdio.h>
int main()
{
int n;     //declaring a variable n.
printf("Enter n : ");
scanf("%d",&n);
if( n%2 == 0){
printf("Your number n is an even number ");
}
else{
printf("Your number n is an odd number ");
}
return 0;
}



